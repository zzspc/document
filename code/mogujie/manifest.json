{
  "manifest_version": 2,
  "name": "蘑菇街抢购",
  "version": "2.0",
  "description": "蘑菇街秒杀拓展",
  "content_scripts": [
    {
      "matches": ["*://shop.mogujie.com/ms/*"],
      "js": ["javascript/jquery-2.1.4.js", "javascript/miaosha.js"]
    }
  ],
  "icons": {
    "16": "images/16.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/19.png",
      "38": "images/38.png"
    },
    "default_title": "蘑菇街抢购",
    "default_popup": "popup.html"
  }
}